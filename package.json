{
  "name": "wdd430-project-url-shortener",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:server": "node server.js",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "format:fix": "prettier --config ./.prettierrc --write \"**/**/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "format:verify": "prettier --config ./.prettierrc --list-different \"**/**/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,js,json,css,scss,html}": [
      "prettier --write ."
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-useragent": "^1.0.15",
    "jquery": "^3.7.0",
    "mongoose": "^7.3.2",
    "morgan": "^1.10.0",
    "rxjs": "~7.8.0",
    "shortid": "^2.2.16",
    "tslib": "^2.3.0",
    "valid-url": "^1.0.9",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular/cli": "~15.2.6",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "~4.3.0",
    "husky": "^8.0.3",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "typescript": "~4.9.4"
  }
}
